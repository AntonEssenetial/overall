.module__page-vid
  padding: 0 0 30px 0;
  background: #f2f2f2;
      
  .iframe-rapper
    position relative
    padding-bottom 56.25% /* 16:9 */
    padding-top 25px
    height 0
  .iframe-rapper iframe
    position absolute
    top 0
    left 0
    width 100%
    height 100%  
    
  iframe 
    margin 0 0 30px 0
    display: block
    overflow hidden
